{
  "private": true,
  "scripts": {
    "start": "env-cmd dotnet run",
    "run:server": "env-cmd dotnet run -p src/Server",
    "reload:server": "docker-compose down -v && docker-compose up -d deps && wait-on tcp:7474 -t 10000 && npm run run:server",
    "run:bgmin": "env-cmd dotnet run -p src/Server bgminute",
    "run:bgday": "env-cmd dotnet run -p src/Server bgdaily",
    "run:generate": "env-cmd dotnet run -p src/Server generate",
    "start:frontend": "webpack-dev-server",
    "build": "webpack -p",
    "test:live": "webpack-dev-server --config webpack.tests.config.js"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "copy-webpack-plugin": "^6.1.0",
    "css-loader": "^5.2.6",
    "env-cmd": "^10.1.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.10.0",
    "open-sans-fonts": "^1.5.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "remotedev": "^0.2.9",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.36.0",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "wait-on": "^6.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-pwa-manifest": "^4.3.0",
    "whatwg-fetch": "^3.6.2",
    "workbox-precaching": "^6.1.5",
    "workbox-webpack-plugin": "^6.1.5"
  },
  "dependencies": {
    "bulma": "0.9.3",
    "bulma-quickview": "2.0.0"
  }
}
