    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "open-sans-fonts": "^1.5.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "remotedev": "^0.2.9",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.36.0",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "wait-on": "^6.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.8.2",
    "webpack-pwa-manifest": "^4.3.0",
    "whatwg-fetch": "^3.6.2",
